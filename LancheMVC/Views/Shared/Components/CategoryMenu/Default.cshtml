@model IEnumerable<LancheMVC_Aplication.DTOs.CategoriaDTO>


<div class="dropdown">
    <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
       Lanches
    </button>
    <ul class="dropdown-menu">
        @foreach(var item in Model){
            <li><a class="dropdown-item" asp-controller="Lanche" asp-action="List" asp-route-Category="@item.CategoryName">@item.CategoryName</a></li>
        
        }
        <li class="dropdown-divider"></li>
        <li><a class="dropdown-item" asp-controller="Lanche" asp-action="List" asp-route-Category="">Todos os lanches</a></li>
    </ul>
</div>